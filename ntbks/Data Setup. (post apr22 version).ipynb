  {
   "cell_type": "code",
   "execution_count": 33,
   "metadata": {},
   "outputs": [],
   "source": [
    "first_diag_age = dict()\n",
    "\n",
    "for ptid in first_diag_date.keys():\n",
    "    \n",
    "    if not ptid in birthdate_data:\n",
    "        print(ptid)\n",
    "        break\n",
    "        continue\n",
    "    \n",
    "    if birthdate_data[ptid] is None:\n",
    "        print(ptid)\n",
    "        break\n",
    "    \n",
    "    first_diag_age[ptid] = (first_diag_date[ptid]-birthdate_data[ptid]).days/365.25"
   ]
